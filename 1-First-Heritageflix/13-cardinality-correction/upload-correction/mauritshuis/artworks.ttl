    schema:name "Portret van Johan Maurits (1604-1679), graaf van Nassau-Siegen, stichter van het Mauritshuis"@nl ;
    schema:publisher <https://www.mauritshuis.nl/> ;
    schema:temporal "1668 - 1670 " ;
    schema:temporalCoverage <http://vocab.getty.edu/aat/300021147> .

