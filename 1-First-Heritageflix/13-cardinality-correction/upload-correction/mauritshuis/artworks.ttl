    schema:name "Portret van Paulus Potter (1625-1654)"@nl ;
    schema:publisher <https://www.mauritshuis.nl/> ;
    schema:temporal "1654" ;
    schema:temporalCoverage <http://vocab.getty.edu/aat/300021147> .

<http://data.collectienederland.nl/resource/document/mauritshuis/542> a schema:VisualArtwork ;
