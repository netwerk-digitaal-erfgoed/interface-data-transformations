    schema:isBasedOn <http://data.collectienederland.nl/resource/document/mauritshuis/731> ;
    schema:name "Apollo omringd door de negen muzen"@nl ;
    schema:publisher <https://www.mauritshuis.nl/> ;
    schema:temporal "1743" ;
    schema:temporalCoverage <http://vocab.getty.edu/aat/300021147> .

