    schema:name "Portret van Maria van Oranje (1642-1688), met Hendrik van Nassau-Zuylestein (overleden in 1673) en een bediende"@nl ;
    schema:publisher <https://www.mauritshuis.nl/> ;
    schema:temporal "1665" ;
    schema:temporalCoverage <http://vocab.getty.edu/aat/300021147> .

