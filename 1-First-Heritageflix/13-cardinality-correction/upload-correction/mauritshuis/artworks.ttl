    schema:publisher <https://www.mauritshuis.nl/> ;
    schema:temporal "1618" ;
    schema:temporalCoverage <http://vocab.getty.edu/aat/300021147> .

<http://data.collectienederland.nl/resource/document/mauritshuis/128> a schema:VisualArtwork ;
